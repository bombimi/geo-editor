{
    "name": "geo-editor",
    "version": "1.0.0",
    "main": "index.js",
    "author": "Martin Slater",
    "license": "Copyright 2025 Martin Slater",
    "private": true,
    "type": "module",
    "watch": {
        "build:dev": {
            "patterns": [
                "lib"
            ],
            "extensions": [
                "ts,js,json"
            ]
        }
    },
    "scripts": {
        "tsbuild": "tsc",
        "tsbuild:watch": "tsc --watch",
        "clean": "rimraf ./dist/*",
        "build:ci": "yarn run rollup -c",
        "build:dev": "yarn run rollup -c --config-dev",
        "watch:dev": "npm-watch build:dev",
        "serve:dev": "wds --root-dir ./site/ --watch",
        "test": "yarn run test:dev && yarn run test:prod",
        "test:dev": "wtr",
        "test:watch": "wtr --watch",
        "test:prod": "MODE=prod wtr",
        "test:prod:watch": "MODE=prod wtr --watch",
        "codegen": "node ./templates/generate.js "
    },
    "dependencies": {
        "@mapbox/togeojson": "^0.16.2"
    },
    "devDependencies": {
        "@rollup/plugin-commonjs": "^25.0.7",
        "@rollup/plugin-json": "^6.1.0",
        "@rollup/plugin-node-resolve": "^15.2.3",
        "@rollup/plugin-replace": "^5.0.5",
        "@rollup/plugin-terser": "^0.4.4",
        "@rollup/plugin-typescript": "^11.1.5",
        "@shoelace-style/shoelace": "2.19.0",
        "@turf/bbox": "^7.1.0",
        "@turf/bbox-polygon": "^7.1.0",
        "@turf/boolean-point-in-polygon": "^7.1.0",
        "@turf/center": "^7.1.0",
        "@turf/helpers": "^7.1.0",
        "@turf/length": "^7.1.0",
        "@types/chalk": "^2.2.0",
        "@types/deep-diff": "^1.0.2",
        "@types/geojson": "^7946.0.13",
        "@types/google.maps": "^3.54.10",
        "@types/leaflet": "^1.9.0",
        "@types/lodash-es": "^4.17.6",
        "@types/uuid": "^8.3.4",
        "@typescript-eslint/eslint-plugin": "^5.59.2",
        "@typescript-eslint/parser": "^5.59.2",
        "@web/dev-server": "^0.3.0",
        "@web/dev-server-rollup": "^0.5.2",
        "@webcomponents/webcomponentsjs": "^2.6.0",
        "async-mutex": "^0.4.0",
        "deep-diff": "^1.0.2",
        "eslint": "^8.39.0",
        "generate-template-files": "^3.2.1",
        "geo-coordinates-parser": "^1.5.8",
        "json": "^11.0.0",
        "leaflet": "^1.9.4",
        "leaflet-kml": "^1.0.1",
        "leaflet-rotatedmarker": "^0.2.0",
        "leaflet.markercluster": "^1.5.3",
        "leaflet.tilelayer.cached": "https://github.com/bombimi/Leaflet.TileLayer.Cached.git",
        "lit": "^3.2.1",
        "lit-analyzer": "^1.2.1",
        "lit-fontawesome": "^0.1.3",
        "lodash-es": "^4.17.21",
        "loglevel": "^1.8.1",
        "loglevel-plugin-prefix": "^0.8.4",
        "luxon": "^3.5.0",
        "npm-watch": "^0.13.0",
        "prettier": "^3.3.3",
        "prettier-plugin-go-template": "^0.0.15",
        "rollup": "^4.22.5",
        "rollup-plugin-copy": "^3.5.0",
        "rollup-plugin-delete": "^2.1.0",
        "rollup-plugin-filesize": "^10.0.0",
        "rollup-plugin-inject-process-env": "^1.3.1",
        "rollup-plugin-polyfill-node": "^0.13.0",
        "rollup-plugin-sourcemaps": "^0.6.3",
        "rollup-plugin-summary": "^2.0.1",
        "rollup-plugin-workbox": "^8.1.0",
        "togeojson": "^0.16.0",
        "ts-md5": "^1.2.11",
        "tslib": "^2.8.1",
        "typedoc": "^0.24.6",
        "typescript": "^5.7.2",
        "uuid": "^8.3.2"
    }
}
